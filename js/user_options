var Ã¥ = {
  data : {},
  calculate_stripes : function(count){
    var i,j,single_stripe,chunk = count;
      var stripes = [];

      for (i=0,j=data.length; i<j; i+=chunk) {
        single_stripe = this.data.slice(i,i+chunk);
        stripes.push(stripe);
      }
    return stripes;
  },
  stripes : function(){
    if(!stripes){
      var stripes = this.calculate_stripes();
    }
  }
}

